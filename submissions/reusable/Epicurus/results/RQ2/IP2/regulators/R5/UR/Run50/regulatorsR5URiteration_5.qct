set k_max=10;
assume (all_k(k>= 3  and k<= 6  impl ( hcv_cmd_fcs_fps{k} < 1.801 ))) or (all_k(k>= 0  and k<= 3  impl ( beta_adc_deg{k} >= 99.5116 )) and all_k(k>= 3  and k<= 6  impl ( hcv_cmd_fcs_fps{k} >= 1.801 ))) or (all_k(k>= 0  and k<= 6  impl  lcv_cmd_fcs_dps_{k} >= -7.5561*(k-0) +64.3466 )) or (all_k(k>= 0  and k<= 3  impl ( beta_adc_deg{k} < 99.5116 )) and all_k(k>= 0  and k<= 3  impl ( alt_des_ft{k} >= 49.8983 )) and all_k(k>= 3  and k<= 6  impl ( hcv_cmd_fcs_fps{k} >= 1.801 )));
assume lcv_cmd_fcs_dps_>0;
% single control point
% lcv_cmd_fcs_dps_{k}<= lcv_cmd_fcs_dps_{k-1} and lcv_cmd_fcs_dps_{k}>= lcv_cmd_fcs_dps_{k-1};
%assume vtas_adc_kts{k}<= vtas_adc_kts{k-1} and vtas_adc_kts{k}>= vtas_adc_kts{k-1};
abs(lcvdt_cmd_fcs_dps2{k} - lcvdt_cmd_fcs_dps2{k-1}) <= 50;